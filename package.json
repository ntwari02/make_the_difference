{
  "name": "mbappe-backend",
  "version": "1.0.0",
  "description": "Backend for Mbappe project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run prebuild:clean && npm run build:css",
    "heroku-postbuild": "npm run build",
    "migrate": "node scripts/migrate.js",
    "migrate:prune": "node scripts/prune-migrations.js",
    "build:css": "node scripts/build-css.js",
    "build:css:watch": "node scripts/build-css.js && echo 'CSS built - use nodemon for watch mode'",
    "build:check": "node -e \"console.log('Build check passed')\"",
    "setup:database": "node setup_database.js",
    "setup:env": "node scripts/setup-env.js",
    "generate:secrets": "node scripts/generate-secrets.js",
    "seed:help": "node scripts/seed-help-data.js",
    "migrate:pain-point": "node scripts/add-pain-point-column.js",
    "test": "echo 'No tests specified' && exit 0",
    "lint": "echo 'No linting configured' && exit 0",
    "clean": "node -e \"try{require('fs').rmSync('public/css/styles.css',{force:true})}catch(e){}\"",
    "prebuild": "npm run prebuild:clean",
    "prebuild:clean": "node -e \"try{require('fs').rmSync('public/css/styles.css',{force:true})}catch(e){}\"",
    "postbuild": "node -e \"console.log('Build completed successfully!')\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@supabase/supabase-js": "^2.56.0",
    "aos": "^2.3.4",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "glightbox": "^3.3.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "imagesloaded": "^5.0.0",
    "isotope-layout": "^3.0.6",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.8"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
